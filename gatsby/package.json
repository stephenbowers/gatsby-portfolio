{
  "name": "gatsby-portfolio",
  "private": true,
  "description": "The web portfolio of Stephen Bowers",
  "version": "0.1.0",
  "license": "0BSD",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=\"-r esm\" gatsby build",
    "develop": "cross-env NODE_OPTIONS=\"-r esm\" gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "cross-env NODE_OPTIONS=\"-r esm\" gatsby serve",
    "clean": "gatsby clean",
    "netlify": "netlify dev",
    "prebuild": "netlify-lambda install"
  },
  "dependencies": {
    "@fontsource/open-sans": "^4.2.2",
    "@fontsource/staatliches": "^4.2.3",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "esm": "^3.2.25",
    "gatsby": "^3.2.1",
    "gatsby-image": "^3.2.0",
    "gatsby-plugin-image": "^1.2.1",
    "gatsby-plugin-react-helmet": "^4.2.0",
    "gatsby-plugin-sanity-image": "^0.6.0",
    "gatsby-plugin-sharp": "^3.2.1",
    "gatsby-plugin-styled-components": "^4.2.0",
    "gatsby-source-filesystem": "^3.2.0",
    "gatsby-source-sanity": "^7.0.2",
    "gatsby-transformer-sharp": "^3.2.0",
    "netlify-lambda": "^2.0.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.2.0",
    "styled-components": "^5.2.3"
  },
  "devDependencies": {
    "prettier": "2.2.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stephenbowers/gatsby-portfolio"
  }
}
